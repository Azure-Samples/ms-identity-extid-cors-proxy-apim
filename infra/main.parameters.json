{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "apimSku": {
        "value": "${AZURE_APIM_SKU=Developer}"
      },
      "apimSkuCapacity": {
        "value": "${AZURE_APIM_SKU_CAPACITY=0}"
      },
      "apimPublisherEmail": {
        "value": "${AZURE_APIM_PUBLISHER_EMAIL=noreply@microsoft.com}"
      },
      "apimPublisherName": {
        "value": "${AZURE_APIM_PUBLISHER_NAME=n/a}"
      }
    }
}
